	"tabBar": {
		"borderStyle": "black",
		"color": "#999999",
		"selectedColor": "#EC661B",
		"backgroundColor": "#FFFFFF",
		"list": [{
				"pagePath": "pages/index/index",
				"iconPath": "static/image/tabBar/index@2x.png",
				"selectedIconPath": "static/image/tabBar/index_s@2x.png",
				"text": "%tabBar.index%"
			},
			{
				"pagePath": "pages/shop/shop",
				"iconPath": "static/image/tabBar/tab-service.png",
				"selectedIconPath": "static/image/tabBar/tab-service-current.png",
				"text": "%tabBar.shop%"
			},
